{
  "run_list": [
    "base",
    "ruby_build",
    "rbenv::user",
    "xbuild-builds"
  ],
  "rbenv": {
    "user_installs": [
      {
        "user": "akira",
        "rubies": ["1.9.3-p545", "2.0.0-p451", "2.1.0", "2.1.1"],
        "gems": {
          "1.9.3-p545": ["bundler", "rake", "pry"],
          "2.0.0-p451": ["bundler", "rake", "pry"],
          "2.1.0": ["bundler", "rake", "pry"],
          "2.1.1": ["bundler", "rake", "pry"]
        },
        "global": "2.1.1"
      }
    ]
  },
  "xbuild-builds": {
    "rubies": [
      {
        "version": "2.1.0",
        "prefix": "/usr/local/ruby-2.1.0"
      },
      {
        "version": "2.1.1",
        "prefix": "/usr/local/ruby-2.1.1"
      }
    ]
  }
}
